extends ../layout/main
block variable
    -
        var title_page = headMenu[1].childs[3][0]
        var id_page = 'developer'
        var class_page = 'developer'
        var breadcrumbsSubitem = [
            {
                label: headMenu[0].item[0],
                link: headMenu[0].item[1]
            },

        ]
    
block footer
  include ../includes/footer-inner-pages.pug
block meta
  title= title_page
  meta(name='description', content='')
block head
    link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
  script(defer src='./assets/scripts/index.js')
  script(defer src='./assets/scripts/vendors.bundle.js')
  script(defer src='./assets/scripts/form.bundle.js')
  script(defer src='./assets/scripts/smooth_scroll.js')
  script(defer src='./assets/scripts/'+id_page+'.js')
block header

block content
    section.page-first-block
        canvas#c
        picture.page-first-block__bg
            source(srcset="./assets/images/developer/bg.jpg")
            img(alt="#" src="./assets/images/developer/bg.jpg")
        include ../includes/breadcrumbs.pug
        .page-first-block__text-wrap 
            <svg class="page-logo" width="37" height="37" viewBox="0 0 37 37" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path data-path-draw-effect d="M0 37H37V0H0V37ZM33.8975 33.8878H3.10248V3.22742H33.8975V33.8878Z" />
            <path data-path-draw-effect d="M20.294 7.40039V27.9308H16.7573V7.40039H7.39844V31.0804H10.3367V10.5499H13.819V31.0804H23.1778V10.5499H26.7146V31.0804H29.5984V7.40039H20.294Z" />
            </svg>
            h1.page-title(data-gsap-anim-enter) #{headMenu[1].childs[3][0]}
            .page-subtitle.with-line Cityconsult Development
        <svg class="mouse screen screen-icon-scroll" data-icon-scroll xmlns="http://www.w3.org/2000/svg" viewBox="-2 -2 76 130" preserveAspectRatio="xMidYMid meet">
            <g fill="none" fill-rule="evenodd">
                <rect width="70" height="118" x="1.5" y="1.5" stroke="#CFBE97" stroke-width="5" rx="36"/>
                <circle class="scroll" cx="36.5" cy="31.5" r="4.5" fill="#CFBE97"/>
            </g>
            </svg>
        ul.lang.screen__lang
            li(data-animate-links).lang__item.lang__item--active
                a(href="#") uk
            li(data-animate-links).lang__item
                a(href="#") ru
            li(data-animate-links).lang__item
                a(href="#") en
        .decor-social(data-animate-links)
        ul.social.screen__social
            li(data-animate-links).social__item
                a(href="#" rel="nofollow" target="_blank") facebook
            li(data-animate-links).social__item
                a(href="#" rel="nofollow" target="_blank") instagram
            li(data-animate-links).social__item
                a(href="#" rel="nofollow" target="_blank") telegram
        .up-arrow(data-up-arrow)
            <svg width="36" height="70" viewBox="0 0 36 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M18 42L18 2M18 2L24 8M18 2L12 8" stroke="#CFBE97" stroke-width="2"/>
                </svg>
            .text Вгору
    section.page-part
        .double-part-block.reverse.double-part-block__img-decor-square_bottom(data-double-part-block)
            .double-part-block__img
                .double-part-block__img-decor-bg
                    img(alt="#" src="./assets/images/developer/block1-bg.jpg").double-part-block__img-decor-bg
                .double-part-block__img-main
                    img(alt="#" src="./assets/images/developer/block1.jpg").double-part-block__img-main
                a(href="https://cityconsult.com.ua/" target='_blank').double-part-block__img-title 
                    img(alt="#" src='./assets/images/developer/developer.png')
                    <button class="home-content__link link" type="submit" data-btn-submit="">
                        <span class="link__text usn" data-btn-submit-text="">Сайт девелопера</span>
                        <span class="link-ico link-ico--margin"><span class="link-ico__stic bgc--wc"></span>
                        <svg class="icon--arrow link__ico fill--wc" role="presentation">
                        <use xlink:href="#icon-arrow"></use>
                        </svg></span></button>
                .double-part-block__img-decor-square
            .double-part-block__text
                .double-part-block__title Короткий заголовок на декілька рядків
                p ЖК Montreal House – <b>зразковий стандарт сучасного житла</b> в діловому та культурному центрі Києва. <b>Преміальне житло бізнес-класу,</b> унікальна локація.
                p <b> Авторські планування,</b> тераси на верхніх рівнях, підземний паркінг, дитячі ігрові зони, особливе оточення відкривають нові важливості життя <b>у серці столиці.</b>
    section.page-part.double-green
        .double-part-block.double-part-block__img-decor-square_bottom(data-double-part-block)
            .double-part-block__img
                .double-part-block__img-decor-bg
                    img(alt="#" src="./assets/images/developer/block2-bg.jpg").double-part-block__img-decor-bg
                .double-part-block__img-main
                    img(alt="#" src="./assets/images/developer/block2.jpg").double-part-block__img-main
                .double-part-block__img-decor-square
            .double-part-block__text
                .double-part-block__title Короткий заголовок на декілька рядків
                p ЖК Montreal House – <b>зразковий стандарт сучасного житла</b> в діловому та культурному центрі Києва. <b>Преміальне житло бізнес-класу,</b> унікальна локація.
                p <b> Авторські планування,</b> тераси на верхніх рівнях, підземний паркінг, дитячі ігрові зони, особливе оточення відкривають нові важливості життя <b>у серці столиці.</b>
    section.page-part 
        -var developerValues = [['Досвід','<b>Багаторічний досвід</b> у сфері нерухомості',],['Проекти','<b>Великий вибір</b> якісної та перевіреної нерухомості',],['Команда','<b>Понад 50 професійних</b> співробітників у штаті',],['Умови купівлі','<b>Вигідні умови</b> купівлі',],['Сервіс','<b>Індивідуальний підбір</b> нерухомості для кожного',],['Надійність','<b>Повний пакет</b> дозвільних документів і висока якість будівництва',],]
        each el in developerValues
            .developer-value
                img(alt="#" src="./assets/images/developer/dev-value1.jpg").developer-value__img
                .subtitle-with-decor.subtitle-with-decor--l.subtitle-with-decor--white.subtitle-with-decor--left-line #{el[0]}
                .text !{el[1]}
        div.running-text.top-padding Нові важливості / <span class="golden-run">Нові важливості / </span>Нові важливості / Нові важливості / Нові важливості / Нові важливості / Нові важливості /
        div.running-text.backward Montreal House / Montreal House / <span class="golden-run">Montreal House /</span>  / Montreal House / Montreal House / Montreal House / Montreal House / 
    section.page-part.double-green.self-size.developer-last-section
        .subtitle-with-decor.subtitle-with-decor--white.subtitle-with-decor--l Інші проекти забудовника
        .slider(data-swiper-projects)
            <svg class="arrow-next" width="19" height="18" viewBox="0 0 19 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M6.99382e-07 9L18 9M18 9L10.3333 17M18 9L10.3333 1" stroke="#E1DDD2"/>
                </svg>
            <svg class="arrow-prev" width="19" height="18" viewBox="0 0 19 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M6.99382e-07 9L18 9M18 9L10.3333 17M18 9L10.3333 1" stroke="#E1DDD2"/>
                </svg>
            .swiper-wrapper
                -var proj = ['./assets/images/developer/projects/forest-quartal.png','./assets/images/developer/projects/forest-quartal.png','./assets/images/developer/projects/illinsky-house.png','./assets/images/developer/projects/krona-park.png','./assets/images/developer/projects/lavandoviy.png',]
                each el in proj
                    img(alt="#" src=el).swiper-slide
        a(href="#").contact-item__link.button-second.button-second--color-white
            .button-second__icon
                +svg('chair').button-second--map-size
            span.button-second__text.button--aylen
                span обрати
                b квартиру
            span.button-second__logo
                +svg('logo-m')
